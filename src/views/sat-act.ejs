<%- include("partials/header"); %>

<div class="sat-act-page">
  <!-- Title Section -->
  <div class="sat-act-title">
    <h1><span class="blue-text">SAT</span> <span class="vs-circle">vs</span> <span class="green-text">ACT</span></h1>
  </div>

  <!-- Why Does it Matter Section -->
  <div class="why-matter-section">
    <div class="banner">why does it matter?</div>
    <div class="why-matter-content">
      <p>
        <strong>SAT:</strong> It can open doors to college opportunities and scholarships, making it easier to plan your future.
      </p>
      <p>
        <strong>ACT:</strong> It shows colleges what you know and gives you a better shot at scholarships and getting into your dream school.
      </p>
    </div>
  </div>

  <!-- Score Range Section -->
  <div class="score-range-section">
    <div class="banner">score range</div>
    <div class="score-range-content">
      <p><span class="blue-text">400 - 1600</span> (SAT)</p>
      <p><span class="green-text">1 - 36 points</span> (ACT)</p>
    </div>
  </div>

  <!-- Test Information Section -->
  <div class="test-info-section">
    <div class="banner">test information</div>
    <div class="test-info-content">
      <div class="test-info-column">
        <h3>SAT</h3>
        <p><strong>Duration:</strong><br>Without Essay: <br>With Essay:</p>
        <p><strong>Sections:</strong><br>Math: 50%<br>Reading & Writing: 50%<br>+ Optional Essay</p>
        <p><strong>Cost:</strong><br>Without Essay: $49.50<br>With Essay: $64.50</p>
      </div>
      <div class="test-info-column">
        <h3>ACT</h3>
        <p><strong>Duration:</strong><br>Without Writing:<br>With Writing:</p>
        <p><strong>Sections:</strong><br>English: 25%<br>Math: 25%<br>Reading: 25%<br>Science: 25%<br>+ Optional Essay</p>
        <p><strong>Cost:</strong><br>Without Essay: $52<br>With Essay: $68</p>
      </div>
    </div>
  </div>
</div>

<%- include("partials/footer"); %>
